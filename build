#!/bin/zsh

TB_PATH="$HOME/Dropbox (Personal)/PARA/2 Areas/tinybrain"

cd "$TB_PATH"

python3 "$HOME/Dropbox (Personal)/Code/swiki/swiki.py" ./wiki ./ -d

new_changes=$(git status | grep -i "modified")

if [[ ! -z "$new_changes" ]]
then
    git add .
    git commit -m "Update"
    git push origin HEAD
fi

